pipeline {
  agent any
  stages {
    stage('Dev') {
      steps {
        echo 'Hi'
      }
    }

    stage('Built') {
      steps {
        echo 'This is print'
        echo 'Built'
      }
    }

    stage('Test') {
      steps {
        echo 'Test'
      }
    }

    stage('UAT') {
      steps {
        echo 'UAT'
      }
    }

    stage('END') {
      steps {
        echo 'END'
      }
    }

  }
}